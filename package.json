{
  "name": "slate-transcript-editor",
  "version": "0.1.6-alpha.14",
  "description": "A React component to make correcting automated transcriptions of audio and video easier and faster. Using the Slate Editor",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"No test specified\"",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o .out",
    "deploy:ghpages": "npm run build-storybook  && cp .nojekyll .out/.nojekyll && gh-pages -d .out",
    "build:component": "tsc -p tsconfig.build.json",
    "build:dev": "tsc -p tsconfig.build.json --watch",
    "publish:public": "npm run build:component && npm publish ./dist --access public",
    "publish:dry:run": "npm run build:component && npm publish ./dist --dry-run",
    "lint": "prettier --write \"src/**/*.(js|ts|jsx|tsx)\"",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "commit-lint": "pretty-quick --staged"
  },
  "keywords": [],
  "author": "Pietro Passarelli",
  "contributors": [
    {
      "name": "Pietro Passarelli",
      "url": "https://github.com/pietrop"
    }
  ],
  "bugs": {
    "url": "https://github.com/pietrop/slate-transcript-editor/issues"
  },
  "homepage": "https://github.com/pietrop/slate-transcript-editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pietrop/slate-transcript-editor.git"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-alpha.35",
    "@material-ui/icons": "^5.0.0-alpha.35",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.9",
    "docx": "^6.0.3",
    "fontsource-roboto": "^4.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sbd": "^1.0.19",
    "slate": "^0.63.0",
    "slate-history": "^0.62.0",
    "slate-react": "^0.65.0",
    "smpte-timecode": "^1.2.3",
    "stt-align-node": "^1.1.10-alpha.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.0-beta.13",
    "@storybook/addon-essentials": "^6.3.0-beta.13",
    "@storybook/addon-links": "^6.3.0-beta.13",
    "@storybook/addon-storysource": "^6.3.0-beta.13",
    "@storybook/addons": "^6.3.0-beta.13",
    "@storybook/react": "^6.3.0-beta.13",
    "@types/jest": "^26.0.23",
    "cross-env": "^7.0.3",
    "gh-pages": "^2.2.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  }
